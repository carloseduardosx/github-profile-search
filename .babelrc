{
	"sourceMaps": true,
	"presets": [
		"es2015",
        "react",
		"stage-0"
	],
	"plugins": [
		["transform-runtime", {
			"helpers": false,
			"polyfill": false,
			"regenerator": true,
			"moduleName": "babel-runtime"
		}],
		["transform-decorators-legacy"],
		["transform-react-jsx", { "pragma": "h" }]
	]
}
